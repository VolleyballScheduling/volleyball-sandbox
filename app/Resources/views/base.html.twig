<!doctype html>
<!-- paulirish.com/2008/conditional-stylesheets-vs-css-hacks-answer-neither/ -->
<!--[if lt IE 7 ]> <html lang="en" class="no-js ie6"> <![endif]-->
<!--[if IE 7 ]>    <html lang="en" class="no-js ie7"> <![endif]-->
<!--[if IE 8 ]>    <html lang="en" class="no-js ie8"> <![endif]-->
<!--[if IE 9 ]>    <html lang="en" class="no-js ie9"> <![endif]-->
<!--[if (gt IE 9)|!(IE)]><!--> <html lang="en" class="no-js"> <!--<![endif]-->
  <head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1">
    <meta http-equiv="Content-Type" content="text/html; charset=utf-8" />
    <meta http-equiv="Content-Language" content="en" />
    
    <link rel="alternate" type="application/rss+xml" href="/feed/" title="RSS" />
    <title>{% block title %}Volleyball: Scheduling for summer camps{% endblock %}</title>

    <meta name="description" content="summer camp scheduling system">
    <meta name="author" content="david durost">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">

    {{ block('stylesheets') }}
    </head>
    <body>
    {% block body %}
        <div class="container">
            {{ block('header') }}
            {{ block('content') }}
        </div>

        {{ block('footer') }}

        {{ block('javascripts') }}
    {% endblock %}
    </body>
</html>

{% block header %}{% endblock %}
{% block content %}{% endblock %}
{% block footer %}{% endblock %}

{% block stylesheets %}
    <link href="http://fonts.googleapis.com/css?family=Oxygen" rel="stylesheet" type="text/css">
    <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/3.2.0/css/bootstrap.min.css">
    <link href="//maxcdn.bootstrapcdn.com/font-awesome/4.2.0/css/font-awesome.min.css" rel="stylesheet">
    <link href="{{ asset('/bundles/volleyballresource/css/volleyball.css') }}" rel="stylesheet">
{% endblock %}

{% block javascripts %}
    <script src="https://code.jquery.com/jquery-2.1.1.min.js"></script>
    <script src="https://code.jquery.com/ui/1.11.1/jquery-ui.min.js"></script>
    <script src="https://maxcdn.bootstrapcdn.com/bootstrap/3.2.0/js/bootstrap.min.js"></script>
{% endblock %}