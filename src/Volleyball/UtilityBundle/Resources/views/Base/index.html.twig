{% extends ':: base.html.twig' %}

{% block javascripts %}
    {{ parent() }}
    <script type="text/javascript" language="javascript" src="//cdn.datatables.net/1.10-beta.1/js/jquery.dataTables.js"></script>
{% endblock %}

{% block stylesheets %}
    {{ parent() }}
    <link rel="stylesheet" type="text/css" href="//cdn.datatables.net/1.10.0-beta.1/css/jquery.dataTables.css">
    <link href="{{ asset('/bundles/Volleyballutility/css/index.css') }}" rel="stylesheet" />
{% endblock %}

{% block content %}
    {{ block('breadcrumbs') }}
    
    {{ block('main_content') }}
{% endblock %}

{% block breadcrumbs %}
        {% include 'VolleyballUtilityBundle:Base:Breadcrumbs.html.twig' with { 'breadcrumbs': breadcrumbs } %}
{% endblock %}

{% block main_content %}{% endblock %}
