{
    "name": "volleyball/sandbox",
    "license": "MIT",
    "type": "project",
    "description": "A summer camp (and alike) scheduling system.",
    "authors": [
        {
            "name": "David Durost",
            "homepage": "http://daviddurost.net"
        }
    ],
    "repositories": [
        {
            "type": "package",
            "package": {
                "name": "jquery/jquery",
                "version": "2.1.1",
                "dist": {
                    "url": "https://code.jquery.com/jquery-2.1.1.min.js",
                    "type": "file"
                }
            }
        }
    ],
    "require": {
        "php":                                  ">=5.4",

        "symfony/symfony":                      "~2.4",
        "symfony/assetic-bundle":               "~2.3",
        "symfony/swiftmailer-bundle":           "~2.3",
        "symfony/monolog-bundle":               "~2.4",
        "symfony/translation":                  "*",
        "symfony-cmf/routing-bundle":           "1.3.*@dev",
        
        "doctrine/orm":                         "~2.3",
        "doctrine/doctrine-bundle":             "1.2.0 as 1.3",
        "doctrine/data-fixtures":               "1.0.x-dev",
        "doctrine/doctrine-fixtures-bundle":    "2.2.*",
        
        "twig/extensions":                      "1.0.*",
        
        "incenteev/composer-parameter-handler": "~2.0",
        
        "sensio/distribution-bundle":           "~2.3",
        "sensio/framework-extra-bundle":        "~3.0",
        "sensio/generator-bundle":              "~2.3",
        
        "jms/aop-bundle":                       "1.0.1",
        "jms/security-extra-bundle":            "1.5.*@dev",
        "jms/di-extra-bundle":                  "1.4.*@dev",
        "jms/translation-bundle":               "1.1.*",
        
        "gedmo/doctrine-extensions":            "2.3.x-dev",
        "stof/doctrine-extensions-bundle":      "~1.1@dev",
        
        "friendsofsymfony/user-bundle":         "2.0.*@dev",
        "pugx/multi-user-bundle":               "3.0.*@dev",
        "friendsofsymfony/rest-bundle":         "~1.0",
        
        "whiteoctober/breadcrumbs-bundle":      "dev-master",
        "white-october/pagerfanta-bundle":      "dev-master",
        "pagerfanta/pagerfanta":                "dev-master",
        
        "braincrafted/bootstrap-bundle":        "1.4.2",
        "twitter/bootstrap":                    "2.3.*",
        "jquery/jquery":                        "2.1.1",
        "leafo/lessphp":                        "dev-master",
        
        "vich/uploader-bundle":                 "dev-master",
        
        "knplabs/knp-menu":                     "2.0.x-dev",
        "knplabs/knp-menu-bundle":              "dev-master",     
        
        "liip/imagine-bundle":                  "dev-master",
        
        "sylius/resource-bundle":               "dev-master",
        "sylius/resource":                      "dev-master"
    },
    "require-dev": {
        "elao/web-profiler-extra-bundle":       "dev-master",
        "jms/debugging-bundle":                 "dev-master",
        "doctrine/doctrine-fixtures-bundle":    "2.2.*",
        "fzaninotto/faker":                     "1.5.*@dev",
        "phpspec/phpspec":                      "~2.0",
        "phpunit/phpunit":                      "~3.7"
    },
    "config": {
        "bin-dir": "bin"
    },
    "minimum-stability": "alpha",
    "extra": {
        "symfony-app-dir": "app",
        "symfony-web-dir": "web",
        "incenteev-parameters": {
            "file": "app/config/parameters.yml"
        }
    },
    "autoload": {
        "psr-0": {
            "Volleyball": "src/"
        }
    }
}
